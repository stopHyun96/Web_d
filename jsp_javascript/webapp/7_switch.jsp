<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html lang="ko">
<head>
<meta charset="UTF-8">
<link rel="stylesheet" href="css/input.css">
<title>Custom CheckBox / Radio</title>
</head>
<body>
<h1>Toggle switch</h1>

<label class="switch">
	<input type="checkbox">
	<span class="slider"></span>
</label>

<label class="switch">
	<input type="checkbox" checked="checked">
	<span class="slider"></span>
</label>

<label class="switch">
	<input type="checkbox">
	<span class="slider round"></span>
</label>

<label class="switch">
	<input type="checkbox" checked="checked">
	<span class="slider round"></span>
</label>

</body>
</html>